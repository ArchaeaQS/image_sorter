{
  "name": "image-sorter-frontend",
  "version": "1.0.0",
  "description": "Image Sorter Frontend with Electron, React and TypeScript",
  "main": "dist/main.js",
  "homepage": "./",
  "scripts": {
    "build": "webpack --mode production",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --mode production",
    "start": "npm run build:main && npm run build:renderer && electron .",
    "dev": "npm run build:main && npm run build:renderer && electron . --dev",
    "watch:main": "tsc -p tsconfig.main.json --watch",
    "watch:renderer": "webpack --mode development --watch"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "electron": "^27.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "css-loader": "^6.11.0",
    "electron-builder": "^24.6.4",
    "html-webpack-plugin": "^5.6.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4"
  }
}
